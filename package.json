{
  "name": "skimjs",
  "version": "0.0.16",
  "description": "Skim CLI",
  "keywords": [
    "scheme",
    "language",
    "compiler",
    "interpreter",
    "skim"
  ],
  "author": "muratg",
  "license": "MIT",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/muratg/skim"
  },
  "main": "bin/skim.js",
  "bin": {
    "skim": "bin/skim.js"
  },
  "files": [
    "bin"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "prettier": "^1.18.2"
  },
  "devDependencies": {
    "jest": "^24.9.0"
  },
  "scripts": {
    "build": "SKIM='./bin/skim.js' ./build.sh",
    "build-lkg": "SKIM='./lkg/bin/skim.js' ./build.sh",
    "build-lkg-prev": "SKIM='./lkg/bin_prev/skim.js' ./build.sh",
    "test": "rm -rf ./tests/basic.test.js && node ./bin/skim.js -c ./tests/basic.test.ss && jest ./tests",
    "dev-install-skim": "sudo npm i -g",
    "dev-uninstall-skim": "sudo npm uninstall -g",
    "UPDATE-LKG": "cp ./lkg/bin/* ./lkg/bin_prev && cp ./bin/* ./lkg/bin",
    "RESTORE-LKG": "cp ./lkg/bin/* ./bin && cp ./lkg/bin_prev/* ./lkg/bin",
    "PUBLISH-NPM": "npm publish"
  }
}
